{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint-config-react-app": "^7.0.1",
    "framer-motion": "^12.23.24",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "zustand": "^5.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "analyze": "react-scripts build --stats | npx webpack-bundle-analyzer /dev/stdin",
    "analyze:sm": "npm run build && source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
